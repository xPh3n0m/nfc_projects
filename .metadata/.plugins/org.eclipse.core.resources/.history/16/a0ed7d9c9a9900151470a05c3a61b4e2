package application.model;

import kw.nfc.communication.Utility;

public class Order {
	
	private int orderId;
	private int guestId;
	private int itemId;
	private int numItem;
	private double itemPrice;
	
	public Order(int oid, int gid, int iid, int nI, double iP) {
		orderId = oid;
		guestId = gid;
		itemId = iid;
		numItem = nI;
		itemPrice = iP;
	}

}
